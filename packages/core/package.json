{
  "name": "@rateme/core",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "build": "rimraf dist && rollup -c",
    "start": "nodemon --watch src --ext ts --exec pnpm run build",
    "precommit": "pnpm run format && pnpm run lint",
    "fix": "pnpm run format:fix && pnpm run lint:fix",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier \"src/**/*.ts\" --debug-check",
    "format:fix": "prettier --write \"src/**/*.ts\""
  },
  "exports": {
    "./api/*": {
      "import": "./dist/api/*.mjs",
      "require": "./dist/api/*.cjs.js",
      "types": "./dist/api/*.d.ts"
    },
    "./domain/*": {
      "import": "./dist/domain/*.mjs",
      "require": "./dist/domain/*.cjs.js",
      "types": "./dist/domain/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^20.3.1",
    "eslint": "^9.18.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^11.0.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.31.0",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "peerDependencies": {
    "axios": "^1.7.9",
    "zod": "^3.24.1"
  }
}
