{
  "name": "@rateme/client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "precommit": "pnpm run locale && pnpm run format && pnpm run lint",
    "test": "vitest",
    "tsc": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier \"src/**/*.ts\" --debug-check",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "locale": "pnpm run locale:extract && pnpm run locale:compile",
    "locale:extract": "lingui extract",
    "locale:compile": "lingui compile --typescript",
    "chakra:snippet": "chakra snippet add --outdir ./src/shared/ui"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.5.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@khmilevoi/use-event": "^1.1.4",
    "@lingui/core": "^5.1.2",
    "@lingui/react": "^5.1.2",
    "@reatom/framework": "^3.4.58",
    "@reatom/npm-react": "^3.10.5",
    "@reatom/testing": "^3.4.8",
    "@tanstack/react-router": "^1.97.3",
    "axios": "^1.7.9",
    "chakra-react-select": "^6.0.1",
    "clsx": "^2.1.1",
    "motion": "^12.0.5",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.5.0",
    "@eslint/js": "^9.17.0",
    "@lingui/babel-plugin-lingui-macro": "^5.1.2",
    "@lingui/cli": "^5.1.2",
    "@lingui/vite-plugin": "^5.1.2",
    "@reatom/eslint-plugin": "^3.7.1",
    "@tanstack/router-devtools": "^1.97.3",
    "@tanstack/router-plugin": "^1.97.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.17.0",
    "eslint-plugin-lingui": "^0.9.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "prettier": "^3.0.0",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  }
}
